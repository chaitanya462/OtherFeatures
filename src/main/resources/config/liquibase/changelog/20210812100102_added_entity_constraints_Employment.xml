        <addForeignKeyConstraint baseColumnNames="worker_id"
                                 baseTableName="employment"
                                 constraintName="fk_employment__worker_id"
                                 referencedColumnNames="id"
                                 referencedTableName="worker"/>
    </changeSet>
</databaseChangeLog>
